    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^21.1.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "prettier": "^3.2.5",
    "storybook": "^6.5.16",
    "storybook-dark-mode": "^3.0.0",
