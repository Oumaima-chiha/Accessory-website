{
  "name": "@flash-drop/bo-app",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "start": "yarn start:dev",
    "start:dev": "env-cmd -f .env.development vite",
    "start:prod": "env-cmd -f .env.prod vite",
    "build:dev": "env-cmd -f .env.development vite build",
    "build:qa": "env-cmd -f .env.qa vite build",
    "build:prod": "env-cmd -f .env.prod vite build",
    "preview": "vite preview",
    "prettier:write": "yarn prettier --write **/*.{js,jsx,ts,tsx,json}",
    "lint": "eslint . --ext .js,.ts,.tsx",
    "lint:fix": "yarn lint --fix",
    "storybook": "set NODE_OPTIONS=--openssl-legacy-provider && start-storybook -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage"
  },
